{
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    },
    "env": {
      "node": true,
      "es6": true
    },
    "extends": "google",
    "rules": {
      "require-jsdoc": "off",
      "no-unused-vars": "off",
      "new-cap": "off",
      "no-invalid-this": "off",
      "max-len": "off",
      "guard-for-in": "off",
      "linebreak-style": "off",
      "indent": ["error", 2],
      }
}
